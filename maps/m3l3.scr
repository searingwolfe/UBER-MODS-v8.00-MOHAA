setup:

	removeclass actor
	removeclass TurretGun
	removeclass Health		// 32 entities
	removeclass AmmoEntity		// 14 entities
	removeclass DrivableVehicle
	removeclass VehicleTank
	removeclass VehicleTurretGun
	removeclass EffectEntity	// nebelwerfer explosions
	removeclass TriggerOnce
	removeclass TriggerUseOnce
	removeclass TriggerChangeLevel
	removeclass TriggerClickItem
	removeclass SplinePath		// removes $bomber, $bomberpath[1] and [2], and all connected nodes.
	//removeclass CrateObject	// 20 entities
	//removeclass WindowObject	// 2 entities
	//removeclass Trigger		// somehow removes ScriptModels, ScriptOrigins, Triggers, & others. Do not use this line.
	//removeclass Animate		// somehow removes ScriptModels, ScriptOrigins, Triggers, & others. Do not use this line.

	for(local.e = 0; local.e <= 868; local.e++) // remove some entities that don't have targetnames.
	{
		local.entity = getentity(local.e)
		if(local.entity != NULL)
		{
			if(local.entity.classname == ScriptModel && local.entity.targetname != "t287" && local.entity.targetname != "t288" && local.entity.targetname != "exploderfire")
			{
				local.entity remove // removes $scene0_panzerschreck & $scene0_shell1 to 7.
			}
			else if(local.entity.classname == Trigger)
			{
				local.entity remove // removes $minefield.
			}
			else if(local.entity.classname == Animate)
			{
				local.entity remove // removes destroyed flak88, destroyed panzer, destroyed nebelwerfers, and glowing & solid explosive planting spots.
			}
			else if(local.entity.classname == ScriptSlave && local.entity.targetname != "exploder" && local.entity.targetname != "exploderchunk" && local.entity.targetname != "explodersmashed")
			{
				local.entity remove // removes numerous invisible collision & mask entities for vehicles, tanks, nebelwerfers and other objects.
			}
			else if(local.entity.classname == ScriptOrigin && local.entity.targetname[0] != "t" && local.entity.targetname[1] != "2")
			{
				local.entity remove // removes 133 entities, not including $exploderchunk-related origins.
			}
