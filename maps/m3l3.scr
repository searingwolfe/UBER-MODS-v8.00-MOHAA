wall_explode local.health local.damage local.radius local.exploderint:

	if($exploder == NULL || $exploderfire == NULL || $exploderchunk == NULL || $explodersmashed == NULL || local.exploderint == NIL) { end }
	if(local.exploderint > 4 || local.exploderint < 1) { end }

	local.tank = spawn "vehicles/tigertank.tik" // tank entity serves as a trigger sensor (there's not enough spawnflags) for only detecting rocket explosions & radiusdamage explosions (stuff that only kills tanks).
	if(local.exploderint == 4) { local.tank.origin = ( 3167.758057 2323.583252 0 ) }
	else { local.tank.origin = ( 3898 $exploder[local.exploderint].origin[1] 0 ) }
	local.tank.angles = ( 0 0 0 )
	local.tank.health = local.health
	local.tank.scale = 0.1
	local.tank notsolid // still has a tiny 1x1 solid box in the center; cannot make the .tik file's setsizes 0 0 0, otherwise explosions cannot touch it.
	local.tank hide

	local.tank2 = spawn "vehicles/tigertank.tik"
	if(local.exploderint == 4) { local.tank2.origin = ( 3207.758057 2131.080322 0 ) }
	else { local.tank2.origin = ( 4107 $exploder[local.exploderint].origin[1] 0 ) }
	if(local.exploderint == 4) { local.tank2.angles = ( 0 270 0 ) }
	else { local.tank2.angles = ( 0 0 0 ) }
	local.tank2.health = local.health
	local.tank2.scale = 0.1
	local.tank2 notsolid
	local.tank2 hide

	while(local.tank != NULL && local.tank2 != NULL) { wait 0.1 }

	println("----- Blowing up stone wall [" + local.exploderint + "].")
	if(local.tank != NULL) { local.tank remove }
	if(local.tank2 != NULL) { local.tank2 remove }

	local.explode = spawn script_model
	local.explode.origin = ( $exploder[local.exploderint].origin[0] $exploder[local.exploderint].origin[1] -271 )
	local.explode model "animate//fx_mortar_higgins.tik" //"fx/fx_flak88_explosion.tik"
	local.explode.scale = 1.0 //2.5
	local.explode anim start

	$exploder[local.exploderint] playsound explode_building_large
	if(local.exploderint == 1) { thread global/exploder.scr::explode 3 } // original game's building chunks exploder script
	if(local.exploderint == 2) { thread global/exploder.scr::explode 1 }
	if(local.exploderint == 3) { thread global/exploder.scr::explode 2 }
	if(local.exploderint == 4) { thread global/exploder.scr::explode 4 }
	radiusdamage $exploder[1].origin local.damage local.radius	// place radiusdamage after exploders finish, since the radiusdamage origin is still inside the wall.
	thread jitter_large	

	waitframe
	local.explode remove
	//$exploder[local.exploderint] remove
end

