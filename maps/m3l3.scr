level waittill spawn

	level.round_starttime = 8
	level.rounds_towin = 6
 	level.changesides = 0
	level.bombcamera = 1
	//level.cameradistance = 200 // this can be changed for each "changesides" if a bombsite is inside a small room, so the camera is not inside of a wall

	if(level.changesides == 0) // allies are planting (axis bombsite origins)
	{
  		waitthread global/cyberattack_searchdestroy.scr::bombsite ( 1355 -1998 -271 ) -90	// for search & destroy only, team parameter doesn't matter
  		waitthread global/cyberattack_searchdestroy.scr::bombsite ( -2401 -6002 -166 ) 90			// make sure to include initial bombsites before the while-loop begins.
	}
	if(level.changesides == 1) // axis are planting (allies bombsite origins)
	{
  		waitthread global/cyberattack_searchdestroy.scr::bombsite ( -3372 4124 -93 ) 90
  		waitthread global/cyberattack_searchdestroy.scr::bombsite ( 1508 3028 -230 ) 0
	}

  	thread global/cyberattack_searchdestroy.scr::bomb ( 1110 5123 -211 ) 225 ( 1970 -1617 -319 ) -90 ( 0 0 0 ) 0
	exec global/cyberattack_searchdestroy.scr // begin the main script

//-----------------------------------------------------------------------------

	local.sides = level.changesides	// when the sides change each 3 rounds, old bombsites are removed and 2 ones are added closer to the defending team's spawn.
	while(1)
	{
		if(level.changesides != local.sides)
		{
			waitthread global/cyberattack_searchdestroy.scr::bombsites_remove

			if(level.changesides == 0)
			{
  				waitthread global/cyberattack_searchdestroy.scr::bombsite ( 1355 -1998 -271 ) -90
  				waitthread global/cyberattack_searchdestroy.scr::bombsite ( -2401 -6002 -166 ) 90
			}
			if(level.changesides == 1)
			{
  				waitthread global/cyberattack_searchdestroy.scr::bombsite ( -3372 4124 -93 ) 90
  				waitthread global/cyberattack_searchdestroy.scr::bombsite ( 1508 3028 -230 ) 0
			}
			local.sides = level.changesides
		}
		waitframe
	}
end

