nebelwerfer_clip_escape local.dist:

	local.PI = 3.14159265359
	while($player.size < 1) { waitframe }
	while($player.size >= 1)
	{
		for(local.i = 1; local.i <= $player.size; local.i++)
		{
			if($player[local.i].drivingent != NIL && $player[local.i].drivingent != NULL)
			{
				if($player[local.i].drivingent.targetname == "nebelwerfer_healthtrig") { $player[local.i].nebelwerfer_clip_escape = 1 }
			}
			else if($player[local.i].nebelwerfer_clip_escape == 1)
			{
				local.degrees = $player[local.i].angles[1]
				if(local.degrees >= 180) { local.degrees = local.degrees - 360 }
				local.radians = local.degrees * (local.PI / 180)
				local.sin = (sin local.radians) * local.dist * -1
				local.cos = (cos local.radians) * local.dist * -1 // negative value = going backwards from angle

				$player[local.i].origin += ( local.cos local.sin 0 )
				$player[local.i].nebelwerfer_clip_escape = 0
			}
		}
		waitframe
	}
	thread nebelwerfer_clip_escape
end

