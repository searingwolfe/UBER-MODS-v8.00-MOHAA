// piper cub
static_piperplane
{
	qer_editorimage textures/models/vehicles/piper_cub/pipercub.tga
	{
		map textures/models/vehicles/piper_cub/pipercub.tga
		rgbGen static
	}
}

// piperplane window
static_piperplane_w
{
	qer_editorimage textures/models/vehicles/piper_cub/pipercub_window.tga
	cull none
	{
		map textures/models/vehicles/piper_cub/pipercub_window.tga
		rgbGen static
		blendFunc blend
		depthWrite
	}
}