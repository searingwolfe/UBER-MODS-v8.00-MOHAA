// King tiger tank
ktigertank
{
	qer_editorimage textures/models/vehicles/ktigertank/ktigertank.tga
	{
		map textures/models/vehicles/ktigertank/ktigertank.tga
		rgbGen lightingGrid
	}
}

static_ktigertank
{
	qer_editorimage textures/models/vehicles/ktigertank/ktigertank.tga
	{
		map textures/models/vehicles/ktigertank/ktigertank.tga
		rgbGen static
	}
}

// King tiger tank treads
ktredz
{
	qer_editorimage textures/models/vehicles/ktigertank/ktredz.tga
	noMerge
	{
		map textures/models/vehicles/ktigertank/ktredz.tga
		rgbGen lightingGrid
		tcmod scroll 0.2 0
	}
}

static_ktredz
{
	qer_editorimage textures/models/vehicles/ktigertank/ktredz.tga
	{
		map textures/models/vehicles/ktigertank/ktredz.tga
		rgbGen vertex
	}
}

