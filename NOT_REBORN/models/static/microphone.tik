TIKI
setup
{
	scale 0.52			// Set default scale to 16/30.5 since world is in 16 units per foot and model is in cm's
	path models/static/microphone
	skelmodel microphone.skd
	surface microphone shader static_microphone
}

init
{
	server
	{
		classname scriptmodel
		setsize "-3 -3 0" "3 3 16"
	}
}

animations
{
	idle     microphone.skc
}

